<h1>All Articles</h1>
<p>
	<%= link_to "All tags", tags_path %>
</p>


<ul id="articles">
  <% @articles.each do |article| %>
    <li>
      <%= link_to article.title, article_path(article) %>
    </li>
  <% end %>
</ul>
<% if logged_in? %>
<%= link_to "Create new article", new_article_path, class: "new_article" %>
<% end %>
